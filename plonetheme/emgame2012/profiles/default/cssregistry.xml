<?xml version="1.0"?>
<object name="portal_css">

    <!-- Set conditions on stylesheets we don't want to pull in -->
    <stylesheet
        id="columns.css"
        expression="not:request/HTTP_X_THEME_ENABLED | nothing"
        />
    <stylesheet
        id="public.css"
        expression="not:request/HTTP_X_THEME_ENABLED | nothing"
        />
    <stylesheet
        id="IEFixes.css"
        expression="not:request/HTTP_X_THEME_ENABLED | nothing"
        />
    <stylesheet id="member.css" enabled="False" />

    <!-- Add new stylesheets -->
    <stylesheet id="++theme++plonetheme.emgame2012/css/reset.css"
        expression="request/HTTP_X_THEME_ENABLED | nothing"
        applyPrefix="True" />

    <stylesheet id="++theme++plonetheme.emgame2012/css/columns.css"
        expression="request/HTTP_X_THEME_ENABLED | nothing"
        applyPrefix="True" />

    <stylesheet id="++theme++plonetheme.emgame2012/css/main.css"
        expression="request/HTTP_X_THEME_ENABLED | nothing"
        applyPrefix="True" />

    <stylesheet id="++theme++plonetheme.emgame2012/css/chosen.css"
        expression="request/HTTP_X_THEME_ENABLED | nothing"
        applyPrefix="True" />


</object>
